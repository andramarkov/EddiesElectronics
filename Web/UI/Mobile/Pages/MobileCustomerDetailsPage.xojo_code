#tag Events Address
	#tag Event
		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  // Show the address in the Maps app
		  // Only using City and State because the
		  // native Maps app has some issues with some
		  // of the addresses in this demo database, reporting
		  // them as "Not found".
		  ShowURL("http://maps.apple.com/maps?q=" + _
		  CurrentRecord.Field("city").StringValue + ", " + _
		  CurrentRecord.Field("state").StringValue)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Phone
	#tag Event
		Sub MouseDown(X As Integer, Y As Integer, Details As REALbasic.MouseEvent)
		  // Call the phone number
		  ShowURL("tel:" + Me.Text)
		End Sub
	#tag EndEvent
#tag EndEvents
