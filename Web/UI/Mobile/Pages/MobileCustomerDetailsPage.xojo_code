#tag WindowCode
	#tag Event
		Sub Shown()
		  // Get the current record
		  CurrentRecord = Session.Orders.FindCustomersByID(Session.HashTag)
		  
		  // Save a cookie for the ID so that it can be automatically
		  // redisplayed if the About or Search page is displayed from
		  // a bookmark or being saved to the home screen
		  // This gets cleared if the user manually navigates to the search
		  // page.
		  Dim expireDate As New Date
		  expireDate.Day = expireDate.Day + 7 // One week
		  Session.Cookies.Set("CustID", Session.HashTag, expireDate)
		  
		  // Get the values from the currentrecord recordset and load them into the user interface controls
		  customerName.Text = CurrentRecord.Field("firstname").StringValue + " " + CurrentRecord.Field("lastname").StringValue
		  
		  // Get the customer photo and scale it down to the size of the Photo control
		  Dim originalPhoto As Picture = CurrentRecord.Field("photo").PictureValue
		  Dim SmallPhoto As New Picture(photo.Width, photo.Height, 32)
		  SmallPhoto.Graphics.DrawPicture(originalPhoto, 0, 0, Photo.Width, Photo.Height, 0, 0, originalPhoto.Width, originalPhoto.Height)
		  Photo.Picture = SmallPhoto
		  
		  // Load the rest of the customer information into the user interface
		  Address.Text = CurrentRecord.Field("address").StringValue + EndOfLine + CurrentRecord.Field("city").StringValue + ", "
		  Address.Text = Address.text + CurrentRecord.Field("state").StringValue + " " + CurrentRecord.Field("zip").StringValue
		  Phone.Text = CurrentRecord.Field("phone").StringValue
		  Email.Text = CurrentRecord.Field("email").StringValue
		  
		  TaxableSwitch.Value = CurrentRecord.Field("taxable").BooleanValue
		  
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
		CurrentRecord As RecordSet
	#tag EndProperty


#tag EndWindowCode

#tag Events AddressLabel
	#tag Event
		Sub Open()
		  If Session.Platform = WebSession.PlatformType.AndroidPhone Then
		    Me.Text = Me.Text.Uppercase
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
